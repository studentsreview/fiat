{
	"name": "perfect-next",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"deploy": "cp -r gh-pages/. out && node deploy.js",
		"export": "next export",
		"format": "prettier --single-quote --write './**/*'",
		"format:check": "prettier --single-quote --check './**/*'",
		"lint": "eslint \"./**/*.{js,ts,tsx,jsx}\" --quiet --fix",
		"lint:check": "eslint \"./**/*.{js,ts,tsx,jsx}\" --quiet",
		"check": "yarn run format:check && yarn run lint:check",
		"check:fix": "yarn run format && yarn run lint"
	},
	"dependencies": {
		"@geist-ui/react": "^2.1.5",
		"@geist-ui/react-icons": "^1.0.1",
		"framer-motion": "^4",
		"gh-pages": "^3.1.0",
		"graphql": "^15.5.0",
		"graphql-request": "^3.4.0",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"next": "10.0.0",
		"react": "17.0.1",
		"react-autosuggest": "^10.1.0",
		"react-dom": "17.0.1",
		"react-icons": "^4.2.0",
		"values.js": "^2.0.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.6",
		"@types/react": "^16.9.55",
		"@types/react-autosuggest": "^10.1.3",
		"@typescript-eslint/eslint-plugin": "^4.1.0",
		"@typescript-eslint/parser": "^4.1.0",
		"eslint": "^7.9.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-wesbos": "^1.0.0",
		"eslint-plugin-html": "^6.1.0",
		"eslint-plugin-import": "2.20.0",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "7.18.0",
		"eslint-plugin-react-hooks": "1.7.0",
		"husky": "^4.3.0",
		"prettier": "^2.1.1",
		"prettier-eslint": "^11.0.0",
		"typescript": "^4.0.5"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn run check:fix"
		}
	}
}
